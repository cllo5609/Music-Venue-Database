<div id="add">
    <form id="add-request">
        <legend><b>Add an Instrument Request</b></legend>
          <fieldset class="fields">
            <label> Instrument ID: </label> <input type="number" name="ins_id"> <br>
            <label> Artist ID: </label> <input type="number" name="artist_id">
          </fieldset>
          <input class="btn" type="submit" id="add_request" value="Add Request">
          <input class="btn" type="button" value="Cancel">
	</form>
</div>
<br>

<div id="update">
    <form id="update-request">
      <legend><b>Update Request</b></legend>
          <fieldset class="fields">
            <label> Instrument ID: </label> <input type="number" name="ins_id">
            <label> Artist ID: </label> <input type="number" name="artist_id">
    </fieldset>
    <input class="btn" type="submit" id="update-request" value="Update Request">
    <input class="btn" type="button" value="Cancel" >
  </form>
</div>
<br>

<div id="delete">
    <form id="delete-request">
      <legend><b>Delete Request</b></legend>
          <fieldset class="fields">
            <label> Event ID </label> <input type="number" name="id">
    </fieldset>
    <input class="btn" type="submit" id="delete-request" value="Delete Request">
    <input class="btn" type="button" value="Cancel" >
  </form>
</div>